{
  "name": "hepsiburada.oms.fulfilment.backoffice.ui",
  "version": "1.0.0",
  "description": "Hepsiburada OMS Fulfilment Backoffice User Interface",
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "http://git.hepsiburada.com/oms/OMS.Fulfilment.Backoffice.UI"
  },
  "keywords": [
    "hepsiburada",
    "oms",
    "fulfilment",
    "backoffice"
  ],
  "author": "omsteam@hepsiburada.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=Development webpack-dev-server --mode development --display-error-details --progress --config webpack.dev.js",
    "dev:qa": "cross-env NODE_ENV=QA webpack-dev-server --mode development --display-error-details --progress --config webpack.dev.js",
    "dev:prod": "cross-env NODE_ENV=Production webpack-dev-server --mode development --display-error-details --progress --config webpack.dev.js",
    "dev:prod:ssl": "cross-env NODE_ENV=Production webpack-dev-server --mode development --display-error-details --progress --https --config webpack.dev.js",
    "build": "webpack-cli -p --progress --display-error-details --verbose --mode production --config webpack.prod.js",
    "lint": "npm run eslint && npm run prettier",
    "prettier": "prettier app/**/*.js app/**/*.scss --write --config .prettierrc.js",
    "eslint": "eslint app/**/*.js --fix",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": [
      "npm run lint",
      "git add"
    ]
  },
