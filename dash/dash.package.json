    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "extract-css-chunks-webpack-plugin": "4.7.4",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.2.0",
    "husky": "4.2.5",
    "lint-staged": "10.1.3",
    "node-sass": "4.13.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.4",
