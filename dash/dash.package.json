    "react-router-dom": "5.1.2",
    "react-scroll": "1.7.16",
    "react-simply-carousel": "^2.1.9",
    "react-spring": "8.0.27",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "0.13.5"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
