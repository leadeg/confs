    "sass-loader": "8.0.2",
    "sass-resources-loader": "2.0.1",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "thread-loader": "2.1.3",
    "url-loader": "^2.1.0",
    "webpack": "4.42.1",
    "webpack-bundle-analyzer": "3.6.1",
