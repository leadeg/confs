{
  "name": "hepsiburada.hermes.productreviews",
  "version": "1.0.0",
  "description": "Product Reviews",
  "main": "app/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=Development webpack-dev-server --mode development --display-error-details --progress --config webpack.development.config.js",
    "build": "webpack-cli -p --progress --display-error-details --verbose --mode production --config webpack.production.config.js",
    "lint": "npm run eslint && npm run prettier",
    "prettier": "prettier app/**/*.jsx app/**/*.js app/**/*.scss --write --config .prettierrc.js",
    "eslint": "eslint app/**/*.jsx app/**/*.js --fix",
    "precommit": "lint-staged",
    "svg:convert": "svgr -d ./app/components/svg ./app/assets/images/svg",
    "svg:index": "create-index ./app/components/svg",
    "svgToReact": "npm run svg:convert && npm run svg:index"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,scss,sass,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://git.hepsiburada.com"
  },
  "keywords": [
    "hepsiburada",
    "hermes",
    "product",
    "reviews",
    "ratings"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hb/Cosmos": "^1.0.15",
    "axios": "0.18.0",
