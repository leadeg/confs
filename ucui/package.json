    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react": "16.8.4",
    "react-alice-carousel": "1.15.2",
    "react-dom": "16.8.4",
    "react-hot-loader": "4.8.0",
    "react-infinite-scroll-component": "4.5.2",
    "react-modal": "3.8.1",
    "react-redux": "6.0.1",
    "react-router": "5.0.0",
    "react-router-dom": "5.0.0",
